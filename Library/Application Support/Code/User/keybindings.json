// Place your key bindings in this file to overwrite the defaults
[
  //-------- Terminal --------
  {
    "key": "ctrl+cmd+h",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+cmd+l",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+cmd+.",
    "command": "workbench.action.terminal.focus"
  },
  {
    "key": "ctrl+cmd+t",
    "command": "workbench.action.terminal.new"
  },
  {
    "key": "ctrl+cmd+x",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+cmd+space",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  //-------- Panels --------
  {
    "key": "ctrl+shift+o",
    "command": "workbench.action.output.toggleOutput"
  },
  {
    "key": "ctrl+shift+p",
    "command": "workbench.actions.view.problems"
  },
  {
    "key": "cmd+\\",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  //-------- Editor Window --------
  {
    "key": "cmd+-",
    "command": "workbench.action.evenEditorWidths"
  },
  {
    "key": "cmd+=",
    "command": "workbench.action.maximizeEditor",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+cmd+h",
    "command": "workbench.action.focusPreviousGroup",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+cmd+l",
    "command": "workbench.action.focusNextGroup",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+\\",
    "command": "workbench.action.splitEditor"
  },
  //-------- Sidebar --------
  {
    "key": "ctrl+shift+d",
    "command": "workbench.view.debug"
  },
  {
    "key": "ctrl+shift+e",
    "command": "workbench.view.explorer"
  },
  {
    "key": "ctrl+shift+x",
    "command": "workbench.view.extensions"
  },
  {
    "key": "ctrl+shift+g",
    "command": "workbench.view.git"
  },
  {
    "key": "ctrl+shift+f",
    "command": "workbench.view.search"
  },
  //-------- Intellisense --------
  {
    "key": "shift+alt+space",
    "command": "editor.action.triggerParameterHints",
    "when": "editorTextFocus"
  },
  //-------- Basic Editing --------
  {
    "key": "alt+a",
    "command": "extension.align",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+l",
    "command": "editor.action.selectHighlights",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+s",
    "command": "editor.action.sortLinesAscending"
  },
  {
    "key": "shift+alt+s",
    "command": "editor.action.sortLinesDescending"
  },
  {
    "key": "ctrl+h",
    "command": "cursorLeft",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+j",
    "command": "cursorDown",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+k",
    "command": "cursorUp",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+l",
    "command": "cursorRight",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+u",
    "command": "scrollPageUp"
  },
  {
    "key": "ctrl+d",
    "command": "scrollPageDown"
  },
  {
    "key": "ctrl+alt+left",
    "command": "subwordNavigation.cursorSubwordLeft",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+h",
    "command": "subwordNavigation.cursorSubwordLeft",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+right",
    "command": "subwordNavigation.cursorSubwordRight",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+l",
    "command": "subwordNavigation.cursorSubwordRight",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+alt+h",
    "command": "subwordNavigation.cursorSubwordLeftSelect",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+alt+l",
    "command": "subwordNavigation.cursorSubwordRightSelect",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+alt+left",
    "command": "subwordNavigation.cursorSubwordLeftSelect",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+alt+right",
    "command": "subwordNavigation.cursorSubwordRightSelect",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+backspace",
    "command": "subwordNavigation.deleteSubwordLeft",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+delete",
    "command": "subwordNavigation.deleteSubwordRight",
    "when": "editorTextFocus"
  },
  //-------- Unbind Commands --------
  {
    "key": "cmd+f2",
    "command": "-editor.action.changeAll",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+cmd+a",
    "command": "-extension.align",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+j",
    "command": "-editor.unfoldAll",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+k",
    "command": "-editor.action.defineKeybinding",
    "when": "editorTextFocus && !editorReadonly && editorLangId == 'json'"
  },
  {
    "key": "cmd+\\",
    "command": "-workbench.action.splitEditor"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "shift+cmd+=",
    "command": "-workbench.action.zoomIn"
  },
  {
    "key": "cmd+=",
    "command": "-workbench.action.zoomIn"
  },
  {
    "key": "shift+cmd+-",
    "command": "-workbench.action.zoomOut"
  },
  {
    "key": "cmd+-",
    "command": "-workbench.action.zoomOut"
  },
  {
    "key": "shift+cmd+d",
    "command": "-workbench.view.debug"
  },
  {
    "key": "shift+cmd+m",
    "command": "-workbench.actions.view.problems"
  },
  {
    "key": "shift+cmd+e",
    "command": "-workbench.view.explorer"
  },
  {
    "key": "shift+cmd+x",
    "command": "-workbench.view.extensions",
    "description": "test"
  },
  {
    "key": "ctrl+shift+g",
    "command": "-workbench.view.git"
  },
  {
    "key": "shift+cmd+f",
    "command": "-workbench.view.search"
  },
  {
    "key": "ctrl+k",
    "command": "-deleteAllRight",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+p",
    "command": "-cursorUp",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+h",
    "command": "-deleteLeft",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+u",
    "command": "-extension.changeCase",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+u",
    "command": "-workbench.action.output.toggleOutput"
  },
  {
    "key": "ctrl+d",
    "command": "-deleteRight",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+alt+j",
    "command": "-extension.slackUploadFileCurrent"
  },
  {
    "key": "ctrl+alt+u",
    "command": "-extension.slackSendSelection"
  },
  {
    "key": "ctrl+alt+j",
    "command": "-extension.slackUploadFileCurrent"
  },
  {
    "key": "shift+alt+a",
    "command": "-editor.action.blockComment",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+delete",
    "command": "-deleteRight",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+j",
    "command": "-workbench.action.togglePanel"
  }
]